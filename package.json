{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite",
    "build": "npm run client:build && npm run server:build",
    "client:build": "vite build",
    "server:build": "tsx server/production.ts",
    "start": "node dist/server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "indexnow": "tsx scripts/submit-indexnow.ts",
    "test": "npm run test:jest",
    "test:jest": "jest",
    "test:coverage": "jest --coverage",
    "subscription:check": "tsx scripts/check-user-subscription.ts",
    "subscription:audit": "tsx scripts/audit-subscription-tiers.ts",
    "validate-security": "tsx scripts/validate-security.ts",
    "postinstall": "npm run validate-security || echo 'Security validation failed - please review configuration'",
    "check": "tsc",
    "seo:update": "node scripts/update-sitemap.js update",
    "seo:validate": "node scripts/update-sitemap.js validate",
    "prebuild": "npm run seo:update",
    "lint:check": "eslint client/src --ext .ts,.tsx",
    "a11y:lint": "eslint client/src --ext .ts,.tsx --fix",
    "a11y:check": "eslint client/src --ext .ts,.tsx",
    "a11y:test": "node scripts/test-accessibility.js",
    "security:verify": "node scripts/verify-security.js",
    "security:audit": "npm audit && npm outdated",
    "security:scan": "npx tsx scripts/security-audit.ts",
    "security:snyk": "snyk test --json > snyk-report.json || true",
    "security:monitor": "snyk monitor",
    "security:semgrep": "semgrep --config=auto --json --quiet .",
    "security:nuclei": "./nuclei -u http://localhost:5000 -severity critical,high,medium",
    "security:zap": "curl -s http://localhost:8080/JSON/core/view/version/ && echo 'ZAP is running' || echo 'Start ZAP first'",
    "subscription:init": "node scripts/init-subscriptions.js",
    "db:setup-local": "tsx scripts/setup-local-db.ts",
    "db:status": "tsx scripts/check-db-status.ts",
    "subscription:init:prod": "node scripts/init-subscriptions.js --production",
    "subscription:add-inactive": "node scripts/add-inactive-status.js",
    "subscription:migrate-free": "node scripts/update-free-tier-status.js",
    "subscription:test-starter": "node scripts/test-starter-simple.js",
    "tokens:test": "node scripts/test-replit-tokens.js",
    "tokens:test-multidevice": "tsx scripts/test-multi-device.js",
    "indexnow:help": "node scripts/submit-indexnow.js help",
    "ipfs:deploy": "node scripts/nft-storage-deploy.js deploy",
    "ipfs:status": "node scripts/nft-storage-deploy.js status",
    "deploy:ipfs": "npm run build && npm run ipfs:deploy",
    "blog:setup": "tsx scripts/setup-blog-database.ts",
    "blog:generate": "curl -X POST http://localhost:5000/api/blog/admin/scheduler/trigger -H 'x-subscription-token: YOUR_ADMIN_TOKEN'",
    "llm:start": "node scripts/local-llm-server.cjs",
    "llm:setup": "npx tsx scripts/setup-nodejs-llm.ts",
    "dev:with-llm": "concurrently \"npm run llm:start\" \"npm run dev\"",
    "crypto:test": "tsx scripts/test-hybrid-crypto.ts"
  },
